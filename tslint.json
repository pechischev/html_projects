{
  "extends": [
    "tslint:all",
    "tslint-react"
  ],
  "linterOptions": {
    "typeCheck": false
  },
  "rules": {
    "align": [
      true,
      "parameters",
      "arguments"
    ],
    "ban": false,
    "class-name": true,
    "curly": true,
    "eofline": false,
    "forin": true,
    "interface-name": [
      true,
      "always-prefix"
    ],
    "jsdoc-format": true,
    "label-position": true,
    "linebreak-style": [
      true,
      "CRLF"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "indent": [true, "tabs", 4],
    "prefer-function-over-method": false,
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-null-keyword": false,
    "newline-per-chained-call": false,
    "no-any": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "no-arg": true,
    "no-console": [
      true,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "arrow-return-shorthand": false,
    "max-line-length": [false],
    "ordered-imports": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "newline-before-return": false,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-shadowed-variable": false,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "one-line": [
      true,
      "check-catch",
      "check-whitespace"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": false,
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}